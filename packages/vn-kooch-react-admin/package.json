{
    "name": "vn-kooch-react-admin",
    "version": "2.7.5",
    "description": "A frontend Framework for building admin applications on top of REST services, using ES6, React and Material UI",
    "files": [
        "*.md",
        "lib",
        "esm",
        "src",
        "docs"
    ],
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "authors": [
        "Fran√ßois Zaninotto"
    ],
    "repository": "marmelab/vn-kooch-react-admin",
    "homepage": "https://github.com/marmelab/vn-kooch-react-admin#readme",
    "bugs": "https://github.com/marmelab/vn-kooch-react-admin/issues",
    "license": "MIT",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "rimraf ./lib && tsc --watch"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "react": "^16.3.0",
        "react-dom": "^16.3.0"
    },
    "dependencies": {
        "vn-kooch-core": "^2.7.5",
        "vn-kooch-language-english": "^2.7.2",
        "vn-kooch-ui-materialui": "^2.7.5"
    },
    "gitHead": "7dff31d0c18edd0e9571c398febfe6891a222630"
}
